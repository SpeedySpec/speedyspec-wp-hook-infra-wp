{
    "name": "speedyspec/speedyspec-wp-hook-infra-wp",
    "version": "1.1.0",
    "description": "SpeedySpec Rewrite of WordPress - Hook API Infrastructure WP Package",
    "type": "library",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=8.4",
        "speedyspec/speedyspec-wp-hook-domain": "^1"
    },
    "require-dev": {
        "pestphp/pest": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "SpeedySpec\\WP\\Hook\\Infra\\WP\\": "src/"
        },
        "files": [
            "classes/class-wp-hook.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SpeedySpec\\WP\\Hook\\Infra\\WP\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "archive": {
        "exclude": [
            "/docs",
            "/tests",
            "/.github",
            "/.git",
            "/.editorconfig",
            "/.gitattributes",
            "/.gitignore",
            "/phpunit.xml",
            "/CHANGELOG.md",
            "/.release-please-manifest.json",
            "/release-please-config.json"
        ]
    }
}
